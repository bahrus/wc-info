{
  "name": "wc-info",
  "version": "0.0.126",
  "description": "Display Web Component Information based on [custom element manifest file](https://github.com/webcomponents/custom-elements-manifest).",
  "main": "wc-info.js",
  "module": "wc-info.js",
  "exports": {
    ".": "./wc-info-base.js",
    "./wc-info-base.js": "./wc-info-base.js",
    "./wc-info-fetch.js": "./wc-info-fetch.js",
    "./wc-info-package-base.js": "./wc-info-package-base.js"
  },
  "keywords": [
    "web-components",
    "custom-elements",
    "custom-elements-manifest"
  ],
  "files": [
    "wc*.*",
    "*.css",
    "types.d.ts",
    "custom-elements.json"
  ],
  "types": "types.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node node_modules/xtal-test/serve.js",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze"
  },
  "dependencies": {
    "xtal-element": "0.0.452",
    "trans-render": "0.0.427",
    "be-decorated": "0.0.33",
    "be-definitive": "0.0.9",
    "be-hive": "0.0.8",
    "be-reformable": "0.0.12",
    "be-repeated": "0.0.17",
    "be-observant": "0.0.33",
    "be-switched": "0.0.21",
    "on-to-me": "0.0.49",
    "xtal-fetch": "0.0.108",
    "pass-down": "0.0.117",
    "ib-id": "0.0.79",
    "if-diff": "0.0.117",
    "d-fine": "0.0.19"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.51",
    "custom-elements-manifest": "1.0.0",
    "@custom-elements-manifest/analyzer": "0.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/wc-info.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/wc-info/issues"
  },
  "homepage": "https://github.com/bahrus/wc-info#readme",
  "customElements": "custom-elements.json"
}
