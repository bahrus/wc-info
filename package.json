    "be-reformable": "0.0.17",
    "be-repeated": "0.0.29",
    "be-switched": "0.0.41",
    "on-to-me": "0.0.49"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.52",
    "custom-elements-manifest": "1.0.0",
    "@custom-elements-manifest/analyzer": "0.5.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/wc-info.git"
  },
